        android:id="@+id/v_dot_2"
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:background="@drawable/shape_dot_status_r"
        app:layout_constraintBottom_toTopOf="@+id/v_line_2"
        app:layout_constraintEnd_toEndOf="@+id/v_line_2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/v_line_2"
        app:layout_constraintTop_toBottomOf="@+id/v_line_1" />

    <View
        android:id="@+id/v_line_2"
        android:layout_width="2dp"
        android:layout_height="0dp"
        android:background="#ffffb89c"
        app:layout_constraintBottom_toTopOf="@+id/v_dot_3"
        app:layout_constraintEnd_toEndOf="@+id/v_dot_3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/v_dot_3"
        app:layout_constraintTop_toBottomOf="@+id/v_dot_2" />

    <View
        android:id="@+id/v_dot_3"
        android:layout_width="10dp"
        android:layout_height="10dp"
