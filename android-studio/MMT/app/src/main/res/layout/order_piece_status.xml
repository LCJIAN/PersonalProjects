        android:layout_height="0dp"
        android:background="#ffffb89c"
        app:layout_constraintBottom_toTopOf="@+id/v_dot_6"
        app:layout_constraintEnd_toEndOf="@+id/v_dot_6"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/v_dot_6"
        app:layout_constraintTop_toBottomOf="@+id/v_dot_5" />

    <View
        android:id="@+id/v_dot_6"
        android:layout_width="10dp"
