<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tv_statics_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:lineSpacingExtra="3dp"
        android:textColor="#ff333333"
        app:layout_constraintBottom_toTopOf="@+id/tv_statics_4"
        app:layout_constraintEnd_toStartOf="@+id/tv_statics_2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="TextView" />

    <TextView
        android:id="@+id/tv_statics_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:lineSpacingExtra="3dp"
        android:textColor="#ff333333"
        app:layout_constraintBottom_toBottomOf="@+id/tv_statics_1"
        app:layout_constraintEnd_toStartOf="@+id/tv_statics_3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tv_statics_1"
        app:layout_constraintTop_toTopOf="@+id/tv_statics_1"
        tools:text="TextView" />

    <TextView
        android:id="@+id/tv_statics_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:lineSpacingExtra="3dp"
        android:textColor="#ff333333"
        app:layout_constraintBottom_toBottomOf="@+id/tv_statics_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tv_statics_2"
        app:layout_constraintTop_toTopOf="@+id/tv_statics_2"
        tools:text="TextView" />

    <TextView
        android:id="@+id/tv_statics_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:lineSpacingExtra="3dp"
        android:textColor="#ff333333"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tv_statics_5"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_statics_1"
        tools:text="TextView" />

    <TextView
        android:id="@+id/tv_statics_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:lineSpacingExtra="3dp"
        android:textColor="#ff333333"
        app:layout_constraintBottom_toBottomOf="@+id/tv_statics_4"
        app:layout_constraintEnd_toStartOf="@+id/tv_statics_6"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tv_statics_4"
        app:layout_constraintTop_toTopOf="@+id/tv_statics_4"
        tools:text="TextView" />

    <TextView
        android:id="@+id/tv_statics_6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:lineSpacingExtra="3dp"
        android:textColor="#ff333333"
        app:layout_constraintBottom_toBottomOf="@+id/tv_statics_5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tv_statics_5"
        app:layout_constraintTop_toTopOf="@+id/tv_statics_5"
        tools:text="TextView" />
</androidx.constraintlayout.widget.ConstraintLayout>